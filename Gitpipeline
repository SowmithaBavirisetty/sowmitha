pipeline {
    agent any
    stages {
        stage (build) {
            steps {
                echo "This is build stage"
                sh 'exit 1'
            }
        }
        stage (test) {
            steps {
                echo "This is test stage"
            }
        }
        stage (deploy) {
            steps {
                echo "This is deploy stage"
            }
        }
    }
}
